@model IEnumerable<LogisticsProject.Models.Naczepy>

@{
    ViewBag.Title = "Naczepy";
}

<h2><br />Naczepy</h2>

<p>
    @Html.ActionLink("Utwórz", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Numer Rejestracyjny")
        </th>
        <th>
            @Html.DisplayName("Ładowność [kg]")
        </th>
        <th>
            @Html.DisplayName("Długość [m]")
        </th>
        <th>
            @Html.DisplayName("Szerokość [m]")
        </th>
        <th>
            @Html.DisplayName("Wysokość [m]")
        </th>
        <th>
            @Html.DisplayName("Chłodnia")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NrRejestracyjny)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ladownosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dlugosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Szerokosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Wysokosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chlodnia)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "Edit", new { id = item.ID }) |
            @Html.ActionLink("Szczegóły", "Details", new { id = item.ID }) |
            @Html.ActionLink("Usuń", "Delete", new { id = item.ID })
        </td>
    </tr>
}

</table>
